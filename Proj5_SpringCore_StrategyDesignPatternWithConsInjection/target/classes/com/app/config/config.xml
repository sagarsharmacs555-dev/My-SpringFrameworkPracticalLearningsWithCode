<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean class="com.app.beans.Bluedart" id="bluedart" />
	<bean class="com.app.beans.Ekart" id="ekart" />
	<bean class="com.app.beans.Delhivery" id="delhivery" />

	<bean class="com.app.beans.Amazon" id="amazon">

		<constructor-arg name="logisticsService" ref="ekart"></constructor-arg>

	</bean>


</beans>